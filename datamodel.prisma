type User {
  id: ID! @id
  email: String! @unique
  password: String
  username: String @unique
  name: String
}

type Product {
  id: ID! @id
  symbol: String! @unique
  engName: String! @unique
  korName: String! @unique
  board: Board @relation(link: INLINE)
}

type Board {
  id: ID! @id
  product: Product!
}